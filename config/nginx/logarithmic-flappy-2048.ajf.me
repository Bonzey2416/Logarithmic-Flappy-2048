server {
    listen 80;
    listen [::]:80;
    listen 443 ssl;
    listen [::]:443 ssl;
    server_name logarithmic-flappy-2048.ajf.me;

    access_log  /var/log/nginx/logarithmic-flappy-2048.ajf.me.access.log;
    error_log  /var/log/nginx/logarithmic-flappy-2048.ajf.me.error.log;

    root    /var/www/logarithmic-flappy-2048.ajf.me;

    location / {        
        index   index.html;
    }
}
